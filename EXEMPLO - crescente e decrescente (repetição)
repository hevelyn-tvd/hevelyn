programa {
  funcao inicio() {
      inteiro y, x
      escreva("Digite o valor de x: \n")
      leia(x)
      escreva("Digite o valor de y: \n")
      leia(y)

      enquanto(x != y) {
        se(x<y)
        escreva("Crescente \n")
        senao
        escreva("Decrescente \n")

        escreva("Digite o valor de x: \n")
        leia(x)
        escreva("Digite o valor de y: \n")
        leia(y)
      }  
  }
}
