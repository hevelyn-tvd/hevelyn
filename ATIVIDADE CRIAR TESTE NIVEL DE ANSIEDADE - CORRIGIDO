Nome = input('Digite o seu nome')
Idade = int(input('Digite a sua idade'))

P1 = int(input('Tem estado mais isolado ultimamente? 2-muito / 1-medianamente / 0-pouco'))
P2 = int(input('Tem estado irritado ou agitado mais que o normal? 2-muito / 1-medianamente / 0-pouco'))
P3 = int(input('Sente com freqüência a sensação de que algo ruim está para acontecer? 2-muito / 1-medianamente / 0-pouco'))
P3 = int(input('Você percebeu alguma alteração no seu sono, como insônia, dificuldades para dormir? 2-muito / 1-medianamente / 0-pouco'))
P4 = int(input('Costuma usar alguma substância para se acalmar? 2-muito / 1-medianamente / 0-pouco'))
P5 = int(input('Sua memória e agilidade de raciocínio pioraram? 2-muito / 1-medianamente / 0-pouco'))
P6 = int(input('Você está mais triste que o habitual? 2-muito / 1-medianamente / 0-pouco'))
P7 = int(input('Na última semana você se incomodou com Inquietação nas Pernas? 2-muito / 1-medianamente / 0-pouco'))
P8 = int(input('Na última semana você ficou com Medo de que algo de ruim aconteça? 2-muito / 1-medianamente / 0-pouco'))
P9 = int(input('Na última semana você ficou Atordoado(a) ou com Tontura? 2-muito / 1-medianamente / 0-pouco'))
P10 = int(input('Na última semana você se incomodou com Palpitação ou Aceleração do batimento cardíaco? 2-muito / 1-medianamente / 0-pouco'))
P11 = int(input('Na última semana você ficou Nervoso(a)? 2-muito / 1-medianamente / 0-pouco'))
P12 = int(input('Na última semana você ficou com Dificuldade em Respirar? 2-muito / 1-medianamente / 0-pouco'))

Soma = P1+P2+P3+P4+P5+P6+P7+P8+P9+P10+P11+P12
print(Nome, '\n', Idade, 'anos')
if Soma<=5:
    print('Nível baixo de ansiedade')
elif Soma>5 and Soma<15:
    print('Nivel moderado de ansiedade')
elif Soma>15 and Soma<24:
    print('Nivel alto de ansiedade')
elif Soma==24:
    print('SOCORROO!!!!!')
